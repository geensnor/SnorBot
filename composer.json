    "pestphp/pest": "^1.23",
    "laravel/pint": "^1.5",
    "rector/rector": "^1.0",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
